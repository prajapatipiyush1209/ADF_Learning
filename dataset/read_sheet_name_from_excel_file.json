{
	"name": "read_sheet_name_from_excel_file",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Excel_File_Name": {
				"type": "string"
			},
			"Sheet_Name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"sheetName": {
				"value": "@dataset().Sheet_Name",
				"type": "Expression"
			},
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().Excel_File_Name",
					"type": "Expression"
				},
				"fileSystem": "excel-sheet-having-multiple-sheets"
			},
			"firstRowAsHeader": true
		},
		"schema": [
			{
				"name": "Date",
				"type": "String"
			},
			{
				"name": "Product",
				"type": "String"
			},
			{
				"name": "Quantity",
				"type": "String"
			},
			{
				"name": "Price",
				"type": "String"
			},
			{
				"name": "Total",
				"type": "String"
			}
		]
	}
}